<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="radeen.co.AsatirManager" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <feature name="InAppBrowser">
        <param name="wp-package" value="InAppBrowser" />
    </feature>
    <feature name="NetworkStatus">
        <param name="wp-package" value="NetworkStatus" />
    </feature>
    <icon height="99" src="res/icons/wp8/icon-62-tile.png" width="99" />
    <icon height="159" src="res/icons/wp8/icon-173-tile.png" width="159" />
    <name>مدیریت املاک</name>
    <description>نرم افزار جامع دفاتر مشاور املاک</description>
    <author email="info@radeen.co" href="http://radeen.co">ایده پردازان دارین</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="windows-target-version" value="8.0" />
    <preference name="windows-phone-target-version" value="8.0" />
    <preference name="Fullscreen" value="true" />
    <preference name="android-minSdkVersion" value="11" />
    <vs:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
    <vs:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
    <vs:plugin name="org.apache.cordova.network-information" version="0.2.12" />
    <vs:platformSpecificValues>
        <vs:platformSpecificWidget platformName="windows" version="1.0.0.0">
            <vs:name>مدیریت املاک</vs:name>
        </vs:platformSpecificWidget>
    </vs:platformSpecificValues>
</widget>
