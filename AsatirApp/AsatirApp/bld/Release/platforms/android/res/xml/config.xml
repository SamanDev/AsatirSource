<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="radeen.co.AsatirApp" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <name>جستجوی ملک</name>
    <description>موتور جستجوی ملک اساطیر</description>
    <author email="info@radeen.co" href="http://radeen.co">ایده پردازان رادین</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.0" />
    <preference name="windows-phone-target-version" value="8.0" />
    <preference name="Orientation" value="default" />
    <preference name="Fullscreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="android-targetSdkVersion" value="11" />
    <vs:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
    <vs:plugin name="org.apache.cordova.network-information" version="0.2.12" />
    <vs:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
    <vs:platformSpecificValues>
        <vs:platformSpecificWidget platformName="windows" version="1.0.0.0">
            <vs:name>جستجوی ملک</vs:name>
        </vs:platformSpecificWidget>
    </vs:platformSpecificValues>
</widget>
