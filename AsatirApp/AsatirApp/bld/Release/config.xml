<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="radeen.co.AsatirApp" android-versionCode="1" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>جستجوی ملک</name>
  <description>موتور جستجوی ملک اساطیر</description>
  <author href="http://radeen.co" email="info@radeen.co">ایده پردازان رادین</author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.0" />
  <preference name="windows-phone-target-version" value="8.0" />
  <preference name="Orientation" value="default" />
  <preference name="Fullscreen" value="true" />
  <preference name="BackupWebStorage" value="cloud" />
  <preference name="android-targetSdkVersion" value="11" />
  <vs:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
  <vs:plugin name="org.apache.cordova.network-information" version="0.2.12" />
  <vs:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
  <platform name="wp8">
    <icon src="res/icons/wp8/icon-62-tile.png" width="99" height="99" />
    <!-- tile image -->
    <icon src="res/icons/wp8/icon-173-tile.png" width="159" height="159" />
  </platform>
  <vs:platformSpecificValues>
    <vs:platformSpecificWidget platformName="windows" version="1.0.0.0">
      <vs:name>جستجوی ملک</vs:name>
    </vs:platformSpecificWidget>
  </vs:platformSpecificValues>
</widget>